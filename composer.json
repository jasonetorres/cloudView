{
    "name": "jasontorres/cloud-view",
    "description": "GUI to view database info in laravel cloud",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Jasontorres\\CloudView\\": "src/"
        }
    },
    "authors": [
        {
            "name": "jasonetorres",
            "email": "jasontorres585@icloud.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.1",
        "illuminate/support": "^11.0 || ^12.0",  
        "illuminate/database": "^11.0 || ^12.0", 
        "doctrine/dbal": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^10.0",
        "orchestra/testbench": "^9.0 || ^10.0",  
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.0",
        "laravel/laravel": "^11.0 || ^12.0"       
    },
    "extra": {
        "laravel": {
            "providers": [
                "Jasontorres\\CloudView\\CloudviewServiceProvider"
            ]
        }
    }
}
